+    private static final String[] THS_NODE_CHANNELS = new String[9];
+    private static final String[] THS_NODE_CHANNELS_BRIGHTNESS = new String[9];
+    private static final String[] THS_NODE_CHANNELS_CURRENTS = new String[9];

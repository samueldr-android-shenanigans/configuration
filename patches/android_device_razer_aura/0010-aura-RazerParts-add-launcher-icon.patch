 RazerParts/AndroidManifest.xml                   | 1 +
 RazerParts/res/drawable/ic_adaptive_launcher.xml | 5 +++++
 2 files changed, 6 insertions(+)
 create mode 100644 RazerParts/res/drawable/ic_adaptive_launcher.xml

diff --git a/RazerParts/AndroidManifest.xml b/RazerParts/AndroidManifest.xml
index c6dbe01..910d196 100755
--- a/RazerParts/AndroidManifest.xml
+++ b/RazerParts/AndroidManifest.xml
@@ -25,6 +25,7 @@
     <application
         android:directBootAware="true"
         android:label="@string/app_name"
+        android:icon="@drawable/ic_adaptive_launcher"
         android:theme="@style/DeviceSettingsTheme">
 
         <activity
diff --git a/RazerParts/res/drawable/ic_adaptive_launcher.xml b/RazerParts/res/drawable/ic_adaptive_launcher.xml
new file mode 100644
index 0000000..b48b003
--- /dev/null
+++ b/RazerParts/res/drawable/ic_adaptive_launcher.xml
@@ -0,0 +1,5 @@
+<?xml version="1.0" encoding="utf-8"?>
+<adaptive-icon xmlns:android="http://schemas.android.com/apk/res/android">
+    <background android:drawable="@color/device_settings_hp_bg"/>
+    <foreground android:drawable="@drawable/cellphone_cog" />
+</adaptive-icon>
\ No newline at end of file
